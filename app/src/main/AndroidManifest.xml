<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.menuka.loginandregistration">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".StudentProfileActivity" />
        <activity android:name=".SemestersActivity" />
        <activity android:name=".SingleSemesterActivity" />
        <activity android:name=".AddSemesterActivity" />
        <activity android:name=".AddModuleActivity" />
        <activity android:name=".StudentDetailsForm" />
        <activity android:name=".ReceivedFeedbackActivity" />
        <activity android:name=".EditSemesterActivity" />
        <activity android:name=".EditModuleActivity" />
        <activity
            android:name=".NavBar"
            android:label="@string/title_activity_nav_bar"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>